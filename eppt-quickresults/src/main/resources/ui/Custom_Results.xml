<panel id="Custom" font="Tahoma-Italic-12" layout="borderlayout">
	<panel constraints="BorderLayout.NORTH" layout="FlowLayout(FlowLayout.LEFT)">
		<button text="Load List/Tree" ActionCommand="CR_LoadList"
				enabled="true">
		</button>
		<button text="Save List/Tree" ActionCommand="CR_SaveList"
				enabled="true">
		</button>
		<button text="Clear Tree" ActionCommand="CR_ClearTree" enabled="true">
		</button>
	</panel>
	<panel constraints="BorderLayout.CENTER" name="WRIMS" id="WRIMS" layout="BorderLayout"
		   preferredsize="900,600">
	</panel>
	<panel layout="FlowLayout(FlowLayout.LEFT)" constraints="BorderLayout.SOUTH">
		<button text="Help" id="CRHelp" ActionCommand="AC_Help"/>
	</panel>
</panel>